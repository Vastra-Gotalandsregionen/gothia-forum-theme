<root>
	<dynamic-element name='rssFeedURL' type='text' index-type='' repeatable='true'>
		<meta-data>
			<entry name="displayAsTooltip"><![CDATA[true]]></entry>
			<entry name="required"><![CDATA[true]]></entry>
			<entry name="instructions"><![CDATA[Skriv in feedens URL.]]></entry>
			<entry name="label"><![CDATA[Feed - URL]]></entry>
			<entry name="predefinedValue"><![CDATA[]]></entry>
		</meta-data>
		
  		<dynamic-element name='rssFeedLabel' type='text' index-type='' repeatable='false'>
  			<meta-data>
				<entry name="displayAsTooltip"><![CDATA[true]]></entry>
				<entry name="required"><![CDATA[true]]></entry>
				<entry name="instructions"><![CDATA[Skriv in feedets namn.]]></entry>
				<entry name="label"><![CDATA[Feed - namn]]></entry>
