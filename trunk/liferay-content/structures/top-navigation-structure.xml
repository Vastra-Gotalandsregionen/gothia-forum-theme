<?xml version="1.0"?>

<root>
	
	<dynamic-element name="links" type="text" index-type="" repeatable="true">
		<meta-data>
			<entry name="displayAsTooltip"><![CDATA[true]]></entry>
			<entry name="required"><![CDATA[false]]></entry>
			<entry name="instructions"><![CDATA[Type a label for the read-more button.]]></entry>
			<entry name="label"><![CDATA[Link label]]></entry>
			<entry name="predefinedValue"><![CDATA[]]></entry>
		</meta-data>
		<dynamic-element name="internalLink" type="link_to_layout" index-type="" repeatable="false">
			<meta-data>
				<entry name="displayAsTooltip"><![CDATA[true]]></entry>
				<entry name="required"><![CDATA[false]]></entry>
				<entry name="instructions"><![CDATA[Choose layout to which the link goes (or specify external link below).]]></entry>
				<entry name="label"><![CDATA[Link Layout]]></entry>
				<entry name="predefinedValue"><![CDATA[]]></entry>
			</meta-data>
		</dynamic-element>
		<dynamic-element name="externalLink" type="text" repeatable="false">
			<meta-data>
				<entry name="displayAsTooltip"><![CDATA[true]]></entry>
				<entry name="required"><![CDATA[false]]></entry>
				<entry name="instructions"><![CDATA[Specify URL to which the link goes (or choose internal link above).]]></entry>
				<entry name="label"><![CDATA[External link]]></entry>
				<entry name="predefinedValue"><![CDATA[]]></entry>
			</meta-data>
		</dynamic-element>
		<dynamic-element name="linkTarget" type="list" index-type="" repeatable="false">
			<meta-data>
				<entry name="displayAsTooltip"><![CDATA[true]]></entry>
				<entry name="required"><![CDATA[false]]></entry>
				<entry name="instructions"><![CDATA[Choose whether the link is opened in the same window or in a new window (_BLANK)]]></entry>
				<entry name="label"><![CDATA[Link Target]]></entry>
				<entry name="predefinedValue"><![CDATA[]]></entry>
			</meta-data>
			<dynamic-element name="Normal" type="none" index-type="" repeatable="false"></dynamic-element>
			<dynamic-element name="New Window (_BLANK)" type="_BLANK" index-type="" repeatable="false"></dynamic-element>
		</dynamic-element>
	</dynamic-element>
	
	<dynamic-element name="linkSignin" type="text" repeatable="false">
		<meta-data>
			<entry name="displayAsTooltip"><![CDATA[true]]></entry>
			<entry name="required"><![CDATA[false]]></entry>
			<entry name="instructions"><![CDATA[Type a label for the sign in link (url is generated by the system).]]></entry>
			<entry name="label"><![CDATA[Link Label Signin]]></entry>
			<entry name="predefinedValue"><![CDATA[]]></entry>
		</meta-data>
	</dynamic-element>

	<dynamic-element name="linkSignout" type="text" repeatable="false">
		<meta-data>
			<entry name="displayAsTooltip"><![CDATA[true]]></entry>
			<entry name="required"><![CDATA[false]]></entry>
			<entry name="instructions"><![CDATA[Type a label for the sign out link (url is generated by the system).]]></entry>
			<entry name="label"><![CDATA[Link Label Signout]]></entry>
			<entry name="predefinedValue"><![CDATA[]]></entry>
		</meta-data>
	</dynamic-element>
	
	<dynamic-element name="linkProfile" type="text" repeatable="false">
		<meta-data>
			<entry name="displayAsTooltip"><![CDATA[true]]></entry>
			<entry name="required"><![CDATA[false]]></entry>
			<entry name="instructions"><![CDATA[Type a label for the profile link]]></entry>
			<entry name="label"><![CDATA[Link Label Profile]]></entry>
			<entry name="predefinedValue"><![CDATA[]]></entry>
		</meta-data>
		<dynamic-element name="internalLink" type="link_to_layout" index-type="" repeatable="false">
			<meta-data>
				<entry name="displayAsTooltip"><![CDATA[true]]></entry>
				<entry name="required"><![CDATA[false]]></entry>
				<entry name="instructions"><![CDATA[Choose layout to which the profile link goes (or specify external link below).]]></entry>
				<entry name="label"><![CDATA[Link Layout]]></entry>
				<entry name="predefinedValue"><![CDATA[]]></entry>
			</meta-data>
		</dynamic-element>
		<dynamic-element name="help" type="text" repeatable="false">
			<meta-data>
				<entry name="displayAsTooltip"><![CDATA[true]]></entry>
				<entry name="required"><![CDATA[false]]></entry>
				<entry name="instructions"><![CDATA[Type a instructions for the help section for the profile link]]></entry>
				<entry name="label"><![CDATA[Link Help]]></entry>
				<entry name="predefinedValue"><![CDATA[]]></entry>
			</meta-data>
		</dynamic-element>
	</dynamic-element>
	
</root>